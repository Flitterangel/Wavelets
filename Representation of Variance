N = 1024;

t=0:0.01:1;

% t = 7/N + 6*(682:(N-3))/N;

% t=1/(N)+ 1*(1:(N))/N;

v1= (10*t.^3)-(15*t.^4)+(6*t.^5);

y = ((t).^2).*((1-t).^2);

% cnormal2 = 1.205* (1-cos(2*pi*p));

% cnormal3 = 0.968* (0.5*(1-cos(5*pi*r)));

plot(t,v1,t,y),title('Representation of v(x) ')

